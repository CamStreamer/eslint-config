{
    "plugins": ["react-hooks"],
    "extends": ["./index.json", "plugin:react/recommended"],
    "rules": {
        "unused-imports/no-unused-vars": [
            "warn",
            {
                "vars": "all",
                "varsIgnorePattern": "^_",
                "args": "after-used",
                "argsIgnorePattern": "^_"
            }
        ],
        "react/react-in-jsx-scope": "off"
    }
}
